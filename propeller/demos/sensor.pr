external objdef Sensor
{
  int temperature;
}

fn print_warning(int oldt, int t) -> void
{
  if (t > 60000) and (t != oldt)
  {
    prints('thermal zone sensor readout too high: ');
    print(t / 1000);
  }
}

fn init() -> int
{
  Sensor sensor;
  bind(sensor.temperature, print_warning);
  return 0;
}
